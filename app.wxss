.divider{
  height: 20rpx;
  background: #f1f1f1;
  box-shadow: 0rpx 3rpx 2rpx 5rpx #f1f1f1 inset;
}


.h2{
  color: #B22222;
  font-weight: bold;
}
.last_title{
  margin: 30rpx;
}
.last_item{
  border-radius: 10rpx;
  margin: 15rpx 10rpx;
  padding: 10rpx;
  display: flex;
  flex-direction: row;
  box-shadow: 0rpx 3rpx 2rpx 5rpx #f1f1f1;
}
.last_item .user_img{
  height: 120rpx;
  width: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.last_item .last_view{
  width: 78%;
}
.last_item .article_detail{
   display: flex;
   flex-direction: row;
}
.last_item .item_title{
  font-size: 15px;
  color: #B22222;
  width: 100%;
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  
}
.last_item .article_detail{
  margin-top: 30rpx;
}
.last_item .article_detail text{
  font-size: 12px;
  color: #666;
  margin-right: 30rpx;
  margin-left: 9rpx;
}

.arrow{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.arrow::after{
  height: 30rpx;
  width: 30rpx;
  border-top: 1px solid #EDECE8;
  border-right: 1px solid #EDECE8;
  transform: rotate(45deg);
  content: "";
  display: block;
}